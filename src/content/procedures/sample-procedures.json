[
  {
    "id": "colonoscopy-screening",
    "name": {
      "en": "Colonoscopy Screening",
      "zh": "大腸鏡檢查",
      "ja": "大腸内視鏡検査"
    },
    "category": "diagnostic",
    "specialty": "gastroenterology",
    "description": {
      "en": "A diagnostic procedure to examine the large intestine (colon) using a flexible tube with a camera",
      "zh": "使用帶有攝像頭的柔性管檢查大腸的診斷程序",
      "ja": "カメラ付きの柔軟なチューブを使用して大腸を検査する診断手順"
    },
    "purpose": {
      "en": "Screen for colorectal cancer, detect polyps, and diagnose bowel conditions",
      "zh": "篩查結直腸癌、檢測息肉並診斷腸道疾病",
      "ja": "大腸がんのスクリーニング、ポリープの検出、腸疾患の診断"
    },
    "duration": {
      "typical": 30,
      "range": {
        "min": 20,
        "max": 60
      }
    },
    "preparation": {
      "beforeProcedure": {
        "en": [
          "Follow clear liquid diet 24 hours before procedure",
          "Take prescribed bowel preparation medication",
          "Arrange transportation home",
          "Remove jewelry and contact lenses"
        ],
        "zh": [
          "檢查前24小時遵循清流質飲食",
          "服用處方腸道準備藥物",
          "安排回家交通",
          "摘除首飾和隱形眼鏡"
        ],
        "ja": [
          "検査24時間前から透明な液体食を摂取",
          "処方された腸管洗浄薬を服用",
          "帰宅の交通手段を手配",
          "アクセサリーとコンタクトレンズを外す"
        ]
      },
      "medications": {
        "en": [
          "Stop blood thinners 5-7 days before (consult doctor)",
          "Continue essential medications with small sips of water",
          "Avoid iron supplements for 1 week"
        ],
        "zh": [
          "檢查前5-7天停止服用血液稀釋劑（諮詢醫生）",
          "用少量水繼續服用必需藥物",
          "1週內避免鐵補充劑"
        ],
        "ja": [
          "5-7日前に血液希釈薬を中止（医師に相談）",
          "少量の水で必要な薬を継続",
          "1週間鉄サプリメントを避ける"
        ]
      },
      "fasting": {
        "required": true,
        "duration": 8,
        "description": {
          "en": "No solid food 8 hours before procedure, clear liquids until 2 hours before",
          "zh": "檢查前8小時禁食固體食物，檢查前2小時前可飲用透明液體",
          "ja": "検査8時間前から固形物摂取禁止、2時間前まで透明な液体可"
        }
      }
    },
    "procedure": {
      "steps": [
        {
          "step": 1,
          "title": {
            "en": "Patient positioning and sedation",
            "zh": "患者定位和鎮靜",
            "ja": "患者の体位と鎮静"
          },
          "description": {
            "en": "Patient lies on left side, IV sedation administered for comfort",
            "zh": "患者左側臥位，靜脈注射鎮靜劑以提供舒適感",
            "ja": "患者は左側臥位、快適性のためIV鎮静薬を投与"
          },
          "duration": 5
        },
        {
          "step": 2,
          "title": {
            "en": "Colonoscope insertion",
            "zh": "結腸鏡插入",
            "ja": "大腸内視鏡の挿入"
          },
          "description": {
            "en": "Flexible colonoscope is gently inserted through the rectum into the colon",
            "zh": "柔性結腸鏡通過直腸輕柔地插入結腸",
            "ja": "柔軟な大腸内視鏡を直腸から大腸に優しく挿入"
          },
          "duration": 10
        },
        {
          "step": 3,
          "title": {
            "en": "Colon examination",
            "zh": "結腸檢查",
            "ja": "大腸の検査"
          },
          "description": {
            "en": "Doctor examines colon walls for abnormalities, polyps, or signs of disease",
            "zh": "醫生檢查結腸壁是否有異常、息肉或疾病跡象",
            "ja": "医師が大腸壁の異常、ポリープ、疾患の兆候を検査"
          },
          "duration": 15
        },
        {
          "step": 4,
          "title": {
            "en": "Biopsy or polyp removal (if needed)",
            "zh": "活檢或息肉切除（如需要）",
            "ja": "生検またはポリープ除去（必要に応じて）"
          },
          "description": {
            "en": "Tissue samples taken or polyps removed using specialized instruments",
            "zh": "使用專用器械採集組織樣本或切除息肉",
            "ja": "専用器具を使用して組織サンプルを採取またはポリープを除去"
          },
          "duration": 10
        }
      ],
      "anesthesia": {
        "type": "sedation",
        "description": {
          "en": "Conscious sedation with midazolam and fentanyl for patient comfort",
          "zh": "使用咪達唑侖和芬太尼進行意識鎮靜以提供患者舒適感",
          "ja": "患者の快適性のためミダゾラムとフェンタニルによる意識下鎮静"
        }
      },
      "equipment": {
        "en": [
          "Flexible colonoscope with camera",
          "Biopsy forceps",
          "Polypectomy snare",
          "CO2 insufflation system",
          "Monitoring equipment"
        ],
        "zh": [
          "帶攝像頭的柔性結腸鏡",
          "活檢鉗",
          "息肉切除圈套器",
          "二氧化碳充氣系統",
          "監護設備"
        ],
        "ja": [
          "カメラ付き柔軟大腸内視鏡",
          "生検鉗子",
          "ポリペクトミースネア",
          "CO2送気システム",
          "モニタリング機器"
        ]
      }
    },
    "risks": {
      "common": [
        {
          "risk": {
            "en": "Mild abdominal discomfort",
            "zh": "輕度腹部不適",
            "ja": "軽度の腹部不快感"
          },
          "probability": "high",
          "severity": "mild",
          "description": {
            "en": "Temporary bloating and cramping from air insufflation",
            "zh": "由於充氣導致的暫時性腹脹和痙攣",
            "ja": "送気による一時的な腹部膨満と痙攣"
          }
        },
        {
          "risk": {
            "en": "Drowsiness from sedation",
            "zh": "鎮靜引起的嗜睡",
            "ja": "鎮静による眠気"
          },
          "probability": "high",
          "severity": "mild",
          "description": {
            "en": "Temporary drowsiness lasting 2-4 hours after procedure",
            "zh": "檢查後持續2-4小時的暫時性嗜睡",
            "ja": "検査後2-4時間続く一時的な眠気"
          }
        }
      ],
      "serious": [
        {
          "risk": {
            "en": "Perforation",
            "zh": "穿孔",
            "ja": "穿孔"
          },
          "probability": "low",
          "severity": "severe",
          "description": {
            "en": "Rare tear in colon wall requiring surgical repair (1 in 1000 cases)",
            "zh": "罕見的結腸壁撕裂需要手術修復（1000例中1例）",
            "ja": "外科修復が必要な稀な大腸壁の裂傷（1000例中1例）"
          }
        },
        {
          "risk": {
            "en": "Bleeding",
            "zh": "出血",
            "ja": "出血"
          },
          "probability": "low",
          "severity": "moderate",
          "description": {
            "en": "Bleeding after polyp removal, usually stops on its own",
            "zh": "息肉切除後出血，通常會自行停止",
            "ja": "ポリープ除去後の出血、通常は自然に止血"
          }
        }
      ]
    },
    "benefits": {
      "en": [
        "Early detection of colorectal cancer",
        "Prevention through polyp removal",
        "Accurate diagnosis of bowel conditions",
        "Peace of mind with normal results"
      ],
      "zh": [
        "早期發現結直腸癌",
        "通過息肉切除進行預防",
        "準確診斷腸道疾病",
        "正常結果帶來安心"
      ],
      "ja": [
        "大腸がんの早期発見",
        "ポリープ除去による予防",
        "腸疾患の正確な診断",
        "正常結果による安心感"
      ]
    },
    "alternatives": [
      {
        "procedure": {
          "en": "CT Colonography (Virtual Colonoscopy)",
          "zh": "CT結腸造影（虛擬結腸鏡）",
          "ja": "CT大腸造影（バーチャル大腸内視鏡）"
        },
        "description": {
          "en": "Non-invasive CT scan of the colon",
          "zh": "結腸的非侵入性CT掃描",
          "ja": "大腸の非侵襲的CTスキャン"
        },
        "pros": {
          "en": [
            "No sedation required",
            "Lower risk of complications",
            "Faster procedure"
          ],
          "zh": [
            "無需鎮靜",
            "併發症風險較低",
            "檢查時間較短"
          ],
          "ja": [
            "鎮静不要",
            "合併症リスクが低い",
            "検査時間が短い"
          ]
        },
        "cons": {
          "en": [
            "Cannot remove polyps during procedure",
            "May miss small lesions",
            "Requires separate colonoscopy if abnormal"
          ],
          "zh": [
            "檢查過程中無法切除息肉",
            "可能遺漏小病變",
            "如有異常需要單獨進行結腸鏡檢查"
          ],
          "ja": [
            "検査中にポリープ除去不可",
            "小さな病変を見逃す可能性",
            "異常時は別途大腸内視鏡が必要"
          ]
        }
      }
    ],
    "aftercare": {
      "immediate": {
        "en": [
          "Rest in recovery area until sedation wears off",
          "Have someone drive you home",
          "Start with light foods and fluids",
          "Avoid alcohol for 24 hours"
        ],
        "zh": [
          "在恢復區休息直到鎮靜作用消失",
          "請人開車送您回家",
          "從清淡食物和液體開始",
          "24小時內避免飲酒"
        ],
        "ja": [
          "鎮静が覚めるまで回復室で休息",
          "誰かに車で送ってもらう",
          "軽い食事と水分から開始",
          "24時間アルコール摂取を避ける"
        ]
      },
      "recovery": {
        "timeline": [
          {
            "period": "Day 1",
            "activities": {
              "en": [
                "Rest at home",
                "Light diet",
                "Avoid driving"
              ],
              "zh": [
                "在家休息",
                "清淡飲食",
                "避免開車"
              ],
              "ja": [
                "自宅で休息",
                "軽い食事",
                "運転を避ける"
              ]
            }
          },
          {
            "period": "Day 2-3",
            "activities": {
              "en": [
                "Resume normal activities",
                "Normal diet",
                "Return to work if feeling well"
              ],
              "zh": [
                "恢復正常活動",
                "正常飲食",
                "如感覺良好可返回工作"
              ],
              "ja": [
                "通常の活動を再開",
                "通常の食事",
                "体調が良ければ仕事復帰"
              ]
            }
          }
        ],
        "fullRecovery": {
          "duration": 1,
          "description": {
            "en": "Most patients feel completely normal within 24 hours",
            "zh": "大多數患者在24小時內感覺完全正常",
            "ja": "ほとんどの患者は24時間以内に完全に正常に感じる"
          }
        }
      },
      "followUp": {
        "required": true,
        "schedule": {
          "en": "Results discussion within 1-2 weeks, next screening in 10 years if normal",
          "zh": "1-2週內討論結果，如正常則10年後進行下次篩查",
          "ja": "1-2週以内に結果説明、正常であれば10年後に次回スクリーニング"
        }
      },
      "warning_signs": {
        "en": [
          "Severe abdominal pain",
          "Heavy rectal bleeding",
          "Fever over 101°F (38.3°C)",
          "Persistent nausea and vomiting",
          "Signs of dehydration"
        ],
        "zh": [
          "嚴重腹痛",
          "大量直腸出血",
          "發燒超過101°F（38.3°C）",
          "持續噁心和嘔吐",
          "脫水跡象"
        ],
        "ja": [
          "激しい腹痛",
          "大量の直腸出血",
          "101°F（38.3°C）以上の発熱",
          "持続する吐き気と嘔吐",
          "脱水の兆候"
        ]
      }
    },
    "contraindications": {
      "absolute": {
        "en": [
          "Acute diverticulitis",
          "Suspected bowel perforation",
          "Severe heart or lung disease",
          "Pregnancy"
        ],
        "zh": [
          "急性憩室炎",
          "疑似腸穿孔",
          "嚴重心肺疾病",
          "懷孕"
        ],
        "ja": [
          "急性憩室炎",
          "腸穿孔の疑い",
          "重篤な心肺疾患",
          "妊娠"
        ]
      },
      "relative": {
        "en": [
          "Recent heart attack",
          "Severe bleeding disorders",
          "Large abdominal hernia",
          "Poor bowel preparation"
        ],
        "zh": [
          "近期心臟病發作",
          "嚴重出血性疾病",
          "大型腹疝",
          "腸道準備不良"
        ],
        "ja": [
          "最近の心筋梗塞",
          "重篤な出血性疾患",
          "大きな腹部ヘルニア",
          "不十分な腸管洗浄"
        ]
      }
    },
    "cost": {
      "estimated": {
        "min": 800,
        "max": 2000,
        "currency": "USD"
      },
      "factors": {
        "en": [
          "Geographic location",
          "Facility type (hospital vs outpatient)",
          "Insurance coverage",
          "Need for polyp removal",
          "Anesthesia type"
        ],
        "zh": [
          "地理位置",
          "設施類型（醫院vs門診）",
          "保險覆蓋",
          "是否需要息肉切除",
          "麻醉類型"
        ],
        "ja": [
          "地理的位置",
          "施設タイプ（病院vs外来）",
          "保険適用",
          "ポリープ除去の必要性",
          "麻酔タイプ"
        ]
      }
    },
    "cptCode": "45378",
    "icdCodes": ["Z12.11"],
    "references": [
      {
        "title": "Colorectal Cancer Screening Guidelines",
        "authors": ["American Cancer Society"],
        "year": 2023,
        "url": "https://www.cancer.org/cancer/colon-rectal-cancer/detection-diagnosis-staging/screening-guidelines.html"
      }
    ],
    "lastUpdated": "2024-01-15T00:00:00.000Z",
    "reviewedBy": "Dr. Sarah Johnson, Gastroenterologist",
    "images": {
      "diagram": "/images/procedures/colonoscopy-diagram.jpg",
      "during": "/images/procedures/colonoscopy-procedure.jpg"
    }
