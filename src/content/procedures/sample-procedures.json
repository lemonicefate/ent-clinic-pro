  {
    "id": "cardiac-catheterization",
    "name": {
      "en": "Cardiac Catheterization",
      "zh": "心導管檢查",
      "ja": "心臓カテーテル検査"
    },
    "category": "diagnostic",
    "specialty": "cardiology",
    "description": {
      "en": "A procedure to examine heart function and blood vessels using a thin, flexible tube (catheter)",
      "zh": "使用細而柔軟的管子（導管）檢查心臟功能和血管的程序",
      "ja": "細くて柔軟なチューブ（カテーテル）を使用して心機能と血管を検査する手順"
    },
    "purpose": {
      "en": "Diagnose coronary artery disease, evaluate heart function, and guide treatment decisions",
      "zh": "診斷冠狀動脈疾病、評估心臟功能並指導治療決策",
      "ja": "冠動脈疾患の診断、心機能の評価、治療方針の決定"
    },
    "duration": {
      "typical": 60,
      "range": {
        "min": 30,
        "max": 120
      }
    },
    "preparation": {
      "beforeProcedure": {
        "en": [
          "Fast for 6-8 hours before procedure",
          "Arrange transportation home",
          "Inform doctor of all medications",
          "Remove jewelry and dentures"
        ],
        "zh": [
          "檢查前禁食6-8小時",
          "安排回家交通",
          "告知醫生所有藥物",
          "摘除首飾和假牙"
        ],
        "ja": [
          "検査6-8時間前から絶食",
          "帰宅の交通手段を手配",
          "すべての薬物を医師に報告",
          "アクセサリーと義歯を外す"
        ]
      },
      "medications": {
        "en": [
          "Continue heart medications unless instructed otherwise",
          "Stop blood thinners if advised by doctor",
          "Take prescribed pre-medications"
        ],
        "zh": [
          "除非另有指示，否則繼續服用心臟藥物",
          "如醫生建議，停止服用血液稀釋劑",
          "服用處方的預防藥物"
        ],
        "ja": [
          "指示がない限り心臓薬を継続",
          "医師の指示があれば血液希釈薬を中止",
          "処方された前投薬を服用"
        ]
      },
      "fasting": {
        "required": true,
        "duration": 6,
        "description": {
          "en": "No food or drink 6 hours before procedure, except small sips of water with medications",
          "zh": "檢查前6小時禁食禁水，除了服藥時少量水",
          "ja": "検査6時間前から飲食禁止、薬服用時の少量の水は可"
        }
      }
    },
    "procedure": {
      "steps": [
        {
          "step": 1,
          "title": {
            "en": "Access site preparation",
            "zh": "穿刺部位準備",
            "ja": "穿刺部位の準備"
          },
          "description": {
            "en": "Local anesthesia applied to wrist or groin area, insertion site cleaned and draped",
            "zh": "在手腕或腹股溝區域施行局部麻醉，清潔並覆蓋插入部位",
            "ja": "手首または鼠径部に局所麻酔、挿入部位を清拭し覆布"
          },
          "duration": 10
        },
        {
          "step": 2,
          "title": {
            "en": "Catheter insertion",
            "zh": "導管插入",
            "ja": "カテーテル挿入"
          },
          "description": {
            "en": "Thin catheter inserted through artery and guided to the heart using X-ray guidance",
            "zh": "細導管通過動脈插入，在X射線引導下導向心臟",
            "ja": "細いカテーテルを動脈から挿入、X線ガイド下で心臓に誘導"
          },
          "duration": 15
        },
        {
          "step": 3,
          "title": {
            "en": "Contrast injection and imaging",
            "zh": "造影劑注射和成像",
            "ja": "造影剤注入と撮影"
          },
          "description": {
            "en": "Contrast dye injected to visualize coronary arteries and heart chambers",
            "zh": "注射造影劑以顯示冠狀動脈和心腔",
            "ja": "造影剤を注入して冠動脈と心腔を可視化"
          },
          "duration": 20
        },
        {
          "step": 4,
          "title": {
            "en": "Assessment and intervention",
            "zh": "評估和干預",
            "ja": "評価と介入"
          },
          "description": {
            "en": "Doctor evaluates results and may perform angioplasty or stent placement if needed",
            "zh": "醫生評估結果，如需要可進行血管成形術或支架置入",
            "ja": "医師が結果を評価、必要に応じて血管形成術やステント留置"
          },
          "duration": 30
        }
      ],
      "anesthesia": {
        "type": "local",
        "description": {
          "en": "Local anesthesia at catheter insertion site, conscious sedation may be used for comfort",
          "zh": "導管插入部位局部麻醉，可使用意識鎮靜以提供舒適感",
          "ja": "カテーテル挿入部位の局所麻酔、快適性のため意識下鎮静を使用することも"
        }
      },
      "equipment": {
        "en": [
          "Cardiac catheters",
          "Contrast dye",
          "X-ray fluoroscopy machine",
          "Pressure monitoring equipment",
          "Emergency resuscitation equipment"
        ],
        "zh": [
          "心導管",
          "造影劑",
          "X射線透視機",
          "壓力監測設備",
          "緊急復甦設備"
        ],
        "ja": [
          "心臓カテーテル",
          "造影剤",
          "X線透視装置",
          "圧力モニタリング機器",
          "緊急蘇生機器"
        ]
      }
    },
    "risks": {
      "common": [
        {
          "risk": {
